{
  "name": "aep-api",
  "version": "0.0.2",
  "description": "AMDJ Entertainment Platform API",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec npm run babel-node -- src/server.js",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "build": "babel src --out-dir build",
    "lint": "eslint --fix . && echo Lint Successful",
    "test:unit": "cross-env NODE_ENV=testing mocha --recursive --require @babel/register --exit",
    "test": "cross-env NODE_ENV=testing MOCHA_FILE=./test/results.xml mocha --recursive --require @babel/register --reporter mocha-junit-reporter --exit",
    "babel-node": "babel-node --presets=@babel/preset-env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amdj-entertainment/aep-api.git"
  },
  "keywords": [
    "AMDJ",
    "Entertainment",
    "AEP",
    "API"
  ],
  "author": "Anthony Mariotti",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/amdj-entertainment/aep-api/issues"
  },
  "homepage": "https://github.com/amdj-entertainment/aep-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^6.1.4",
    "mocha-junit-reporter": "^1.23.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "convict": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.0.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.13",
    "mongoose-paginate-v2": "^1.3.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "regenerator-runtime": "^0.13.2",
    "winston": "^3.2.1",
    "winston-mongodb": "^5.0.0"
  }
}
